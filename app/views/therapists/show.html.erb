	<h2>Patient List</h2>
       	<div class="table-responsive">
            <table class="table table-striped">
              <thead>
                <tr>
                  <th>First Name</th>
                  <th>Last Name</th>
                  <th>DOB</th>
                </tr>
              </thead>
              <tbody>

                <% @patients.each do |patient| %>
                  <tr>
                    <td><%= patient.FirstName %></td>
                    <td><%= patient.LastName %></td>
                    <td><%= patient.DateOfBirth %></td>
                  </tr>
                <% end %>

              </tbody>
            </table>
        </div>